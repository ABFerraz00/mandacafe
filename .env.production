# Configuração de Produção
# Substitua pelos valores reais em produção

# Banco de Dados PostgreSQL (Neon)
DATABASE_URL="postgresql://username:password@hostname:port/database?sslmode=require"

# Configuração do Servidor
PORT=3000
NODE_ENV=production

# Configuração de CORS (URLs do frontend em produção)
CORS_ORIGIN="https://seu-frontend.vercel.app,https://seu-dominio.com"

# Configuração de Logs
LOG_LEVEL=warn

# Configuração de Segurança (opcional)
JWT_SECRET="seu_jwt_secret_super_seguro_aqui"
API_KEY="sua_api_key_para_admin"

